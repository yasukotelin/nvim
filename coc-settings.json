{
  "diagnostic.errorSign": "E>",
  "diagnostic.infoSign": "I>",
  "diagnostic.warningSign": "W>",
  "diagnostic.hintSign": "H>",
  "diagnostic.messageTarget": "echo",
  "suggest.keepCompleteopt": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "filetypes": [
        "go"
      ]
    }
  }
}
